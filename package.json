{
  "name": "onichan-bot",
  "version": "1.0.0",
  "description": "周防桃子が好きです",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "ts-node ./src/index.ts",
    "compile": "tsc -p .",
