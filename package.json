    "discord.js": "^12.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.21.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12.x.x"
  }
}
